{
  "name": "biblidev",
  "description": "Bibliothèque de ressources utilisées dans le développement web et le web design. Plus besoin de chercher pour trouver son bonheur",
  "author": "Joachim Brasier",
  "homepage": "https://biblidev.fr",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IcezLP/biblidev.git"
  },
  "bugs": {
    "url": "https://github.com/IcezLP/biblidev/issues"
  },
  "scripts": {
    "dev": "concurrently \"nodemon server.js\" \"npm run sass:watch\"",
    "start": "node server.js",
    "build": "npm run sass:build && next build",
    "sass:build": "sass stylesheets/main.scss:public/css/main.css",
    "sass:watch": "sass --watch stylesheets/main.scss:public/css/main.css"
  },
  "dependencies": {
    "@svgr/webpack": "^5.3.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.9.5",
    "next": "^9.3.1",
    "next-seo": "^4.4.0",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.3",
    "validator": "^13.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  }
}
